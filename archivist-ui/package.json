    "electron-webpack": "^2.8.2",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "fuse.js": "^5.2.3",
    "immer": "^6.0.3",
    "lodash": "^4.17.15",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hotkeys-hook": "^2.1.3",
    "react-virtualized": "^9.21.2",
    "source-map-support": "^0.5.19",
