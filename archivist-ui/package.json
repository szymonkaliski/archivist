{
  "name": "archivist-ui",
  "version": "1.1.0",
  "description": "ui for archivist-cli",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder"
  },
  "build": {
    "appId": "com.szymonkaliski.archivist-ui",
